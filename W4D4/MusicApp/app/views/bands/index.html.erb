<h1>Bandsssssssssssssss</h1>

<table>
  <tr>
    <td>
      <form action="<%= new_band_url %>" method="get">
        <input type="submit" value="create new band">
      </form>
    </td>
    <td>
      <form action="<%= new_album_url %>" method="get">
        <input type="submit" value="create new album">
      </form>
    </td>
    <td>
      <form action="<%= new_track_url %>" method="get">
        <input type="submit" value="create new track">
      </form>
    </td>
  </tr>
</table>

<h2>All Bands</h2>
<ul>
<% @bands.each do |band|%>
  <li><a href="<%= band_url(band) %>"><%= band.name %></a></li>
<% end %>
</ul>
