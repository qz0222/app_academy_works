<h1> <%= @cat.name %></h1>
<hr>
<table>
  <tr>
    <td>Age:</td>
    <td><%= @cat.age %></td>
  </tr>
  <tr>
    <td>Sex:</td>
    <td><%= @cat.sex %></td>
  </tr>
  <tr>
    <td>Color:</td>
    <td><%= @cat.color %></td>
  </tr>
  <tr>
    <td>Description:</td>
    <td colspan=2><%= @cat.description %></td>
  </tr>
  <tr height="20">
  </tr>
  <% @cat.cat_rental_requests.each do |request| %>
  <tr>
    <td><%= request.id %>
    </td>
    <td><%= request.start_date %>
    </td>
    <td><%= request.end_date %>
    </td>
    <td><%= request.status %>
    </td>
  </tr>
  <% end %>
</table>
<hr>

<br><br>
<a href="<%= edit_cat_url(@cat) %>">Edit Cat</a>
<br><br>
<a href="<%= cats_url %>">Back to Cats</a>
